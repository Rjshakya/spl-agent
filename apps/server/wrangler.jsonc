{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "sql-agent-server",
	"main": "src/app.ts",
	"compatibility_date": "2026-02-02",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"hyperdrive": [
		{
			"binding": "HYPERDRIVE_DEV",
			"id": "3f651e00445148e08a54bce9b1291b1e",
			"localConnectionString": "postgresql://postgres:postgres@localhost:5432/postgres"
		}
	],
	"kv_namespaces": [
		{
			"binding": "SQL_AGENT_KV",
			"id": "5895a54b64d24abc89276f568c25294a",
			"remote": true
		}
	]
